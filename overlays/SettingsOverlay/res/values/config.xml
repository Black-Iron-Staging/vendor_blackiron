<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android"
           xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Package name and fully-qualified class name for the wallpaper picker activity. -->
    <string name="config_styles_and_wallpaper_picker_action">android.intent.action.SET_WALLPAPER</string>
    <string name="config_styles_and_wallpaper_picker_class">com.android.customization.picker.CustomizationPickerActivity</string>
    <string name="config_wallpaper_picker_action">android.intent.action.MAIN</string>
    <string name="config_wallpaper_picker_class">com.google.android.apps.wallpaper.picker.CategoryPickerActivity</string>
    <string name="config_wallpaper_picker_package">com.google.android.apps.wallpaper</string>

    <!-- Whether to display Cloned Apps page in Settings (Settings > Apps > Cloned Apps).-->
    <bool name="config_cloned_apps_page_enabled">true</bool>
    
    <!-- Settings intelligence package name -->
    <string name="config_settingsintelligence_log_action">com.google.android.settings.intelligence.contextualcards.CARD_INTERACTION_RECEIVER</string>
    
    <!-- Settings intelligence interaction log intent action -->
    <string name="config_settingsintelligence_package_name">com.google.android.settings.intelligence</string>
    
    <!-- Adaptive Connectivity -->
    <bool name="config_show_adaptive_connectivity">true</bool>
    
    <item name="config_activity_embed_split_ratio" format="float" type="dimen">0.5</item>
</resources>
